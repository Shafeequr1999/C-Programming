#include <stdio.h>
#include<conio.h>
#include<string.h>
void main()
{
    char str[100];
    int i;
    printf("Enter any String:\n");
    scanf("%s",str);
   
    for(i=9;i<strlen(str);i++)
    {
    if(str[i]=='a'||str[i]=='A'||str[i]=='e'||str[i]=='E'||str[i]=='i'||str[i]=='I'||str[i]=='o'||str[i]=='O'||str[i]=='u'||str[i]=='U')
    str[i]='*';
    }
    printf("The replaced string %s in place of vowel is:",str);
    getch();
}

